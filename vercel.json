{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "suitability_api.py" }],
  "functions": {
    "suitability_api.py": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "suitability_api.py",
      "headers": {
        "Access-Control-Allow-Origin": "https://upleads-prod.web.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
